r(v.B*255))end return{type="Colorpicker",flag=v;color=c(s.Color)or nil,alpha=s.Alpha}end,Load=function(s,c)local function C(v)local s=tonumber(v:sub(2,3),16)/255 local c=tonumber(v:sub(4,5),16)/255 local C=tonumber(v:sub(6,7),16)/255 return Color3.new(s,c,C)end if v.Options[s]and c.color then v.Options[s]:SetColor(C(c.color))if c.alpha then v.Options[s]:SetAlpha(c.alpha)end end end}}function v.SetFolder(t,s,c,C)if O then return"Config system unavailable."end local E={s,s..("/"..c),s..("/"..(c..("/"..C))),s..("/"..(c..("/"..(C.."/settings"))))}for v,s in ipairs(E)do if not isfolder(s)then makefolder(s)end end v.Folder=s..("/"..(c..("/"..C)))return v.Folder.."/settings"end function v.LoadAutoLoadConfig(s)if O then return"Config system unavailable."end if isfile(v.Folder.."/settings/autoload.txt")then local s=readfile(v.Folder.."/settings/autoload.txt")local c,C=v:LoadConfig(s)if not c then x:Notify({Title=string.rep(" ",3).."Settings Manager",Description="Error loading autoload config: "..C})end x:Notify({Title=string.rep(" ",3).."Settings Manager";Description=string.format("Autoloaded config: %q",s)})end end function v.DeleteConfig(c,s)if O then return"Config system unavailable."end if not s then return false,"Please select a config file."end local C=v.Folder..("/settings/"..(s..".json"))if isfile(C)then delfile(C)else return false,"Config file does not exist."end local t=v.Folder.."/settings/autoload.txt"if isfile(t)then local v=readfile(t)if v==s then delfile(t)x:Notify({Title=string.rep(" ",3).."Settings Manager",Description="Autoload file also deleted as it matched the deleted config."})end end return true end function v.SaveConfig(c,s)if O then return"Config system unavailable."end if not s then return false,"Please select a config file."end local t=v.Folder..("/settings/"..(s..".json"))local E={objects={}}for v,s in next,v.Options do if not rV[s.Class]then continue end if s.IgnoreConfig then continue end table.insert(E.objects,rV[s.Class].Save(v,s))end local A,h=pcall(C.JSONEncode,C,E)if not A then return false,"Unable to encode into JSON data"end writefile(t,h)return true end function v.LoadConfig(c,s)if O then return"Config system unavailable."end if not s then return false,"Please select a config file."end local t=v.Folder..("/settings/"..(s..".json"))if not isfile(t)then return false,"Invalid file"end local E,A=pcall(C.JSONDecode,C,readfile(t))if not E then return false,"Unable to decode JSON data."end for v,s in next,A.objects do if rV[s.type]then task.spawn(function()rV[s.type].Load(s.flag,s)end)end end return true end function v.RefreshConfigList(s)if O then return"Config system unavailable."end local c=listfiles(v.Folder.."/settings")local C={}for v=1,#c,1 do local s=c[v]if s:sub(-5)==".json"then local v=s:find(".json",1,true)local c=v local t=s:sub(v,v)while t~="/"and(t~="\092"and t~="")do v=v-1 t=s:sub(v,v)end if t=="/"or t=="\092"then local t=s:sub(v+1,c-1)if t~="options"then table.insert(C,t)end end end end return C end F.Enabled=false local aV={}for v,s in pairs(D)do table.insert(aV,s)end t:PreloadAsync(aV)F.Enabled=true j=true return x end local function x(v)return string.format("%02i",v)end local function F(v)local s=(v-v%60)/60 v=v-s*60 local c=(s-s%60)/60 s=s-c*60 return x(c)..(":"..(x(s)..(":"..x(v))))end task.spawn(function()if not(getgenv()).ExecutedInfoStats then(getgenv()).ExecutedInfoStats=true while task.wait(.95)do pcall(function()local v=(game:GetService("CoreGui")):FindFirstChild("MacLib")if v then local s=v.Base.Sidebar.SidebarGroup.UserInfo.InformationGroup local c=s.UserAndDisplayFrame:FindFirstChild("DisplayName")local C=s.UserAndDisplayFrame:FindFirstChild("Username")local t=s:FindFirstChild("Headshot")if c and(C and t)then local v=os.date("%b %d, %Y")local s=string.format("%.2f ms",(game:GetService("Stats")).Network.ServerStatsItem["Data Ping"]:GetValue())local E=os.date("%H:%M")c.Text=s..(" | "..(v..(" | "..E)))c.TextSize=11 C.Text="RunTime: "..F(time())t.Visible=false end end end)end end end)return v
