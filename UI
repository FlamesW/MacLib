unction(M,B)if n.Options[M]and B.state~=nil then task.wait(.15)n.Options[M]:UpdateState(B.state)end end},Colorpicker={Save=function(n,M)local function B(n)return string.format("#%02X%02X%02X",math.floor(n.R*255),math.floor(n.G*255),math.floor(n.B*255))end return{type="Colorpicker";flag=n,color=B(M.Color)or nil;alpha=M.Alpha}end;Load=function(M,B)local function O(n)local M=tonumber(n:sub(2,3),16)/255 local B=tonumber(n:sub(4,5),16)/255 local O=tonumber(n:sub(6,7),16)/255 return Color3.new(M,B,O)end if n.Options[M]and B.color then n.Options[M]:SetColor(O(B.color))if B.alpha then n.Options[M]:SetAlpha(B.alpha)end end end}}local function RD()if l then return"Config system unavailable."end local M={n.Folder;n.Folder.."/settings"}for n=1,#M,1 do local B=M[n]if not isfolder(B)then makefolder(B)end end end function n.LoadAutoLoadConfig(M)if l then return"Config system unavailable."end if isfile(n.Folder.."/settings/autoload.txt")then local M=readfile(n.Folder.."/settings/autoload.txt")local B,O=n:LoadConfig(M)if not B then z:Notify({Title=string.rep(" ",3).."Settings Manager";Description="Error loading autoload config: "..O})end z:Notify({Title=string.rep(" ",3).."Settings Manager";Description=string.format("Autoloaded config: %q",M)})end end function n.DeleteConfig(B,M)if l then return"Config system unavailable."end if not M then return false,"Please select a config file."end local O=n.Folder..("/settings/"..(M..".json"))if isfile(O)then delfile(O)else return false,"Config file does not exist."end local b=n.Folder.."/settings/autoload.txt"if isfile(b)then local n=readfile(b)if n==M then delfile(b)z:Notify({Title=string.rep(" ",3).."Settings Manager",Description="Autoload file also deleted as it matched the deleted config."})end end return true end function n.SetFolder(B,M)if l then return"Config system unavailable."end n.Folder=M RD()end function n.SaveConfig(B,M)if l then return"Config system unavailable."end if not M then return false,"Please select a config file."end local b=n.Folder..("/settings/"..(M..".json"))local Y={objects={}}for n,M in next,n.Options do if not QD[M.Class]then continue end if M.IgnoreConfig then continue end table.insert(Y.objects,QD[M.Class].Save(n,M))end local c,D=pcall(O.JSONEncode,O,Y)if not c then return false,"Unable to encode into JSON data"end writefile(b,D)return true end function n.LoadConfig(B,M)if l then return"Config system unavailable."end if not M then return false,"Please select a config file."end local b=n.Folder..("/settings/"..(M..".json"))if not isfile(b)then return false,"Invalid file"end local Y,c=pcall(O.JSONDecode,O,readfile(b))if not Y then return false,"Unable to decode JSON data."end for n,M in next,c.objects do if QD[M.type]then task.spawn(function()QD[M.type].Load(M.flag,M)end)end end return true end function n.RefreshConfigList(M)if l then return"Config system unavailable."end local B=listfiles(n.Folder.."/settings")local O={}for n=1,#B,1 do local M=B[n]if M:sub(-5)==".json"then local n=M:find(".json",1,true)local B=n local b=M:sub(n,n)while b~="/"and(b~="\092"and b~="")do n=n-1 b=M:sub(n,n)end if b=="/"or b=="\092"then local b=M:sub(n+1,B-1)if b~="options"then table.insert(O,b)end end end end return O end d.Enabled=false local FD={}for n,M in pairs(I)do table.insert(FD,M)end b:PreloadAsync(FD)d.Enabled=true q=true return z end local function z(n)return string.format("%02i",n)end local function d(n)local M=(n-n%60)/60 n=n-M*60 local B=(M-M%60)/60 M=M-B*60 return z(B)..(":"..(z(M)..(":"..z(n))))end task.spawn(function()if not(getgenv()).ExecutedInfoStats then(getgenv()).ExecutedInfoStats=true while task.wait(.95)do pcall(function()local n=(game:GetService("CoreGui")):FindFirstChild("MacLib")if n then local M=n.Base.Sidebar.SidebarGroup.UserInfo.InformationGroup local B=M.UserAndDisplayFrame:FindFirstChild("DisplayName")local O=M.UserAndDisplayFrame:FindFirstChild("Username")local b=M:FindFirstChild("Headshot")if B and(O and b)then local n=os.date("%b %d, %Y")local M=string.format("%.2f ms",(game:GetService("Stats")).Network.ServerStatsItem["Data Ping"]:GetValue())local Y=os.date("%H:%M")B.Text=M..(" | "..(n..(" | "..Y)))B.TextSize=11 O.Text="RunTime: "..d(time())b.Visible=false end end end)end end end)return n
