unction(c,f)if m.Options[c]and f.state~=nil then task.wait(.15)m.Options[c]:UpdateState(f.state)end end},Colorpicker={Save=function(m,c)local function f(m)return string.format("#%02X%02X%02X",math.floor(m.R*255),math.floor(m.G*255),math.floor(m.B*255))end return{type="Colorpicker";flag=m,color=f(c.Color)or nil,alpha=c.Alpha}end,Load=function(c,f)local function U(m)local c=tonumber(m:sub(2,3),16)/255 local f=tonumber(m:sub(4,5),16)/255 local U=tonumber(m:sub(6,7),16)/255 return Color3.new(c,f,U)end if m.Options[c]and f.color then m.Options[c]:SetColor(U(f.color))if f.alpha then m.Options[c]:SetAlpha(f.alpha)end end end}}local function Ih()if n then return"Config system unavailable."end local c={m.Folder,m.Folder.."/settings"}for m=1,#c,1 do local f=c[m]if not isfolder(f)then makefolder(f)end end end function m.LoadAutoLoadConfig(c)if n then return"Config system unavailable."end if isfile(m.Folder.."/settings/autoload.txt")then local c=readfile(m.Folder.."/settings/autoload.txt")local f,U=m:LoadConfig(c)if not f then W:Notify({Title=string.rep(" ",3).."Settings Manager";Description="Error loading autoload config: "..U})end W:Notify({Title=string.rep(" ",3).."Settings Manager",Description=string.format("Autoloaded config: %q",c)})end end function m.DeleteConfig(f,c)if n then return"Config system unavailable."end if not c then return false,"Please select a config file."end local U=m.Folder..("/settings/"..(c..".json"))if isfile(U)then delfile(U)else return false,"Config file does not exist."end local l=m.Folder.."/settings/autoload.txt"if isfile(l)then local m=readfile(l)if m==c then delfile(l)W:Notify({Title=string.rep(" ",3).."Settings Manager",Description="Autoload file also deleted as it matched the deleted config."})end end return true end function m.SetFolder(f,c)if n then return"Config system unavailable."end m.Folder=c Ih()end function m.SaveConfig(f,c)if n then return"Config system unavailable."end if not c then return false,"Please select a config file."end local l=m.Folder..("/settings/"..(c..".json"))local x={objects={}}for m,c in next,m.Options do if not Ah[c.Class]then continue end if c.IgnoreConfig then continue end table.insert(x.objects,Ah[c.Class].Save(m,c))end local V,H=pcall(U.JSONEncode,U,x)if not V then return false,"Unable to encode into JSON data"end writefile(l,H)return true end function m.LoadConfig(f,c)if n then return"Config system unavailable."end if not c then return false,"Please select a config file."end local l=m.Folder..("/settings/"..(c..".json"))if not isfile(l)then return false,"Invalid file"end local x,V=pcall(U.JSONDecode,U,readfile(l))if not x then return false,"Unable to decode JSON data."end for m,c in next,V.objects do if Ah[c.type]then task.spawn(function()Ah[c.type].Load(c.flag,c)end)end end return true end function m.RefreshConfigList(c)if n then return"Config system unavailable."end local f=listfiles(m.Folder.."/settings")local U={}for m=1,#f,1 do local c=f[m]if c:sub(-5)==".json"then local m=c:find(".json",1,true)local f=m local l=c:sub(m,m)while l~="/"and(l~="\092"and l~="")do m=m-1 l=c:sub(m,m)end if l=="/"or l=="\092"then local l=c:sub(m+1,f-1)if l~="options"then table.insert(U,l)end end end end return U end Q.Enabled=false local Eh={}for m,c in pairs(a)do table.insert(Eh,c)end l:PreloadAsync(Eh)Q.Enabled=true d=true return W end local function W(m)return string.format("%02i",m)end local function Q(m)local c=(m-m%60)/60 m=m-c*60 local f=(c-c%60)/60 c=c-f*60 return W(f)..(":"..(W(c)..(":"..W(m))))end task.spawn(function()if not(getgenv()).ExecutedInfoStats then(getgenv()).ExecutedInfoStats=true while task.wait(.95)do pcall(function()local m=(game:GetService("CoreGui")):FindFirstChild("MacLib")if m then local c=m.Base.Sidebar.SidebarGroup.UserInfo.InformationGroup local f=c.UserAndDisplayFrame:FindFirstChild("DisplayName")local U=c.UserAndDisplayFrame:FindFirstChild("Username")local l=c:FindFirstChild("Headshot")if f and(U and l)then local m=os.date("%b %d, %Y")local c=string.format("%.2f ms",(game:GetService("Stats")).Network.ServerStatsItem["Data Ping"]:GetValue())local x=os.date("%H:%M")f.Text=c..(" | "..(m..(" | "..x)))f.TextSize=11 U.Text="RunTime: "..Q(time())l.Visible=false end end end)end end end)return m
