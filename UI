e,T,i)if not w then return false,"Unable to encode into JSON data"end writefile(u,N)return true end function q.LoadConfig(U,v)if b then return"Config system unavailable."end if not v then return false,"Please select a config file."end local u=q.Folder..("/settings/"..(v..".json"))if not isfile(u)then return false,"Invalid file"end local i,w=pcall(T.JSONDecode,T,readfile(u))if not i then return false,"Unable to decode JSON data."end for q,v in next,w.objects do if pG[v.type]then task.spawn(function()pG[v.type].Load(v.flag,v)end)end end return true end function q.RefreshConfigList(v)if b then return"Config system unavailable."end local U=listfiles(q.Folder.."/settings")local T={}for q=1,#U,1 do local v=U[q]if v:sub(-5)==".json"then local q=v:find(".json",1,true)local U=q local u=v:sub(q,q)while u~="/"and(u~="\092"and u~="")do q=q-1 u=v:sub(q,q)end if u=="/"or u=="\092"then local u=v:sub(q+1,U-1)if u~="options"then table.insert(T,u)end end end end return T end d.Enabled=false local hG={}for q,v in pairs(t)do table.insert(hG,v)end u:PreloadAsync(hG)d.Enabled=true H=true return V end local function V(q)return string.format("%02i",q)end local function d(q)local v=(q-q%60)/60 q=q-v*60 local U=(v-v%60)/60 v=v-U*60 return V(U)..(":"..(V(v)..(":"..V(q))))end task.spawn(function()if not(getgenv()).ExecutedInfoStats then(getgenv()).ExecutedInfoStats=true while task.wait(.95)do pcall(function()local q=(game:GetService("CoreGui")):FindFirstChild("MacLib")if q then local v=q.Base.Sidebar.SidebarGroup.UserInfo.InformationGroup local U=v.UserAndDisplayFrame:FindFirstChild("DisplayName")local T=v.UserAndDisplayFrame:FindFirstChild("Username")local u=v:FindFirstChild("Headshot")if U and(T and u)then local q=os.date("%b %d, %Y")local v=string.format("%.2f ms",(game:GetService("Stats")).Network.ServerStatsItem["Data Ping"]:GetValue())local i=os.date("%H:%M")U.Text=v..(" | "..(q..(" | "..i)))U.TextSize=11 T.Text="RunTime: "..d(time())u.Visible=false end end end)end end end)return q
