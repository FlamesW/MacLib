/settings"}for v=1,#j,1 do local D=j[v]if not isfolder(D)then makefolder(D)end end end function v.LoadAutoLoadConfig(j)if p then return"Config system unavailable."end if isfile(v.Folder.."/settings/autoload.txt")then local j=readfile(v.Folder.."/settings/autoload.txt")local D,q=v:LoadConfig(j)if not D then Z:Notify({Title=string.rep(" ",3).."Settings Manager";Description="Error loading autoload config: "..q})end Z:Notify({Title=string.rep(" ",3).."Settings Manager",Description=string.format("Autoloaded config: %q",j)})end end function v.DeleteConfig(D,j)if p then return"Config system unavailable."end if not j then return false,"Please select a config file."end local q=v.Folder..("/settings/"..(j..".json"))if isfile(q)then delfile(q)else return false,"Config file does not exist."end local G=v.Folder.."/settings/autoload.txt"if isfile(G)then local v=readfile(G)if v==j then delfile(G)Z:Notify({Title=string.rep(" ",3).."Settings Manager",Description="Autoload file also deleted as it matched the deleted config."})end end return true end function v.SetFolder(D,j)if p then return"Config system unavailable."end v.Folder=j B4()end function v.SaveConfig(D,j)if p then return"Config system unavailable."end if not j then return false,"Please select a config file."end local G=v.Folder..("/settings/"..(j..".json"))local r={objects={}}for v,j in next,v.Options do if not a4[j.Class]then continue end if j.IgnoreConfig then continue end table.insert(r.objects,a4[j.Class].Save(v,j))end local A,g=pcall(q.JSONEncode,q,r)if not A then return false,"Unable to encode into JSON data"end writefile(G,g)return true end function v.LoadConfig(D,j)if p then return"Config system unavailable."end if not j then return false,"Please select a config file."end local G=v.Folder..("/settings/"..(j..".json"))if not isfile(G)then return false,"Invalid file"end local r,A=pcall(q.JSONDecode,q,readfile(G))if not r then return false,"Unable to decode JSON data."end for v,j in next,A.objects do if a4[j.type]then task.spawn(function()a4[j.type].Load(j.flag,j)end)end end return true end function v.RefreshConfigList(j)if p then return"Config system unavailable."end local D=listfiles(v.Folder.."/settings")local q={}for v=1,#D,1 do local j=D[v]if j:sub(-5)==".json"then local v=j:find(".json",1,true)local D=v local G=j:sub(v,v)while G~="/"and(G~="\092"and G~="")do v=v-1 G=j:sub(v,v)end if G=="/"or G=="\092"then local G=j:sub(v+1,D-1)if G~="options"then table.insert(q,G)end end end end return q end M.Enabled=false local f4={}for v,j in pairs(T)do table.insert(f4,j)end G:PreloadAsync(f4)M.Enabled=true E=true return Z end local function Z(v)return string.format("%02i",v)end local function M(v)local j=(v-v%60)/60 v=v-j*60 local D=(j-j%60)/60 j=j-D*60 return Z(D)..(":"..(Z(j)..(":"..Z(v))))end task.spawn(function()if not(getgenv()).ExecutedInfoStats then(getgenv()).ExecutedInfoStats=true while task.wait(.95)do pcall(function()local v=(game:GetService("CoreGui")):FindFirstChild("MacLib")if v then local j=v.Base.Sidebar.SidebarGroup.UserInfo.InformationGroup local D=j.UserAndDisplayFrame:FindFirstChild("DisplayName")local q=j.UserAndDisplayFrame:FindFirstChild("Username")local G=j:FindFirstChild("Headshot")if D and(q and G)then local v=os.date("%b %d, %Y")local j=string.format("%.2f ms",(game:GetService("Stats")).Network.ServerStatsItem["Data Ping"]:GetValue())local r=os.date("%H:%M")D.Text=j..(" | "..(v..(" | "..r)))D.TextSize=11 q.Text="RunTime: "..M(time())G.Visible=false end end end)end end end)return v
