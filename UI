 p.KeyCode==w1 then A1()end end)j.MouseButton1Click:Connect(A1)h.MouseButton1Click:Connect(function()T:Dialog({Title=s.Title,Description="Are you sure you want to exit the menu?",Buttons={{Name="Confirm";Callback=function()T:Unload()end},{Name="Cancel"}}})end)function T.SetKeybind(s,p)w1=p end function T.SetAcrylicBlurState(s,p)x=p f.BackgroundTransparency=p and.15 or 0 end function T.GetAcrylicBlurState(p)return x end local function t1(p)if p then LP.Visible=false MP.Text="Loading Info..."kP.Text=""else LP.Visible=false MP.Text="Loading Info..."kP.Text=""end end local g1 if s.ShowUserInfo~=nil then g1=s.ShowUserInfo else g1=true end t1(g1)function T.SetUserInfoState(s,p)t1(p)end function T.GetUserInfoState(s,p)return g1 end function T.SetSize(s,p)f.Size=p end function T.GetSize(s,p)return f.Size end function T.SetScale(s,p)E.Scale=p end function T.GetScale(p)return E.Scale end local J1={Keybind={Save=function(p,s)return{type="Keybind";flag=p;bind=typeof(s.Bind)=="EnumItem"and s.Bind.Name or nil}end;Load=function(s,m)if p.Options[s]and m.bind then p.Options[s]:Bind(Enum.KeyCode[m.bind])end end},Input={Save=function(p,s)return{type="Input",flag=p;text=s.Text}end,Load=function(s,m)if p.Options[s]and(m.text and type(m.text)=="string")then p.Options[s]:UpdateText(m.text)end end},Slider={Save=function(p,s)return{type="Slider",flag=p,value=s.Value and tostring(s.Value)or false}end,Load=function(s,m)if p.Options[s]and m.value then p.Options[s]:UpdateValue(tonumber(m.value))end end};Dropdown={Save=function(p,s)return{type="Dropdown";flag=p;value=s.Value}end,Load=function(s,m)if p.Options[s]and m.value then p.Options[s]:UpdateSelection(m.value)end end},Toggle={Save=function(p,s)return{type="Toggle",flag=p,state=s.State or false}end,Load=function(s,m)if p.Options[s]and m.state~=nil then task.wait(.15)p.Options[s]:UpdateState(m.state)end end},Colorpicker={Save=function(p,s)local function m(p)return string.format("#%02X%02X%02X",math.floor(p.R*255),math.floor(p.G*255),math.floor(p.B*255))end return{type="Colorpicker",flag=p;color=m(s.Color)or nil;alpha=s.Alpha}end;Load=function(s,m)local function U(p)local s=tonumber(p:sub(2,3),16)/255 local m=tonumber(p:sub(4,5),16)/255 local U=tonumber(p:sub(6,7),16)/255 return Color3.new(s,m,U)end if p.Options[s]and m.color then p.Options[s]:SetColor(U(m.color))if m.alpha then p.Options[s]:SetAlpha(m.alpha)end end end}}local function r1()if i then return"Config system unavailable."end local s={p.Folder;p.Folder.."/settings"}for p=1,#s,1 do local m=s[p]if not isfolder(m)then makefolder(m)end end end function p.LoadAutoLoadConfig(s)if i then return"Config system unavailable."end if isfile(p.Folder.."/settings/autoload.txt")then local s=readfile(p.Folder.."/settings/autoload.txt")local m,U=p:LoadConfig(s)if not m then T:Notify({Title=string.rep(" ",3).."Settings Manager";Description="Error loading autoload config: "..U})end T:Notify({Title=string.rep(" ",3).."Settings Manager";Description=string.format("Autoloaded config: %q",s)})end end function p.DeleteConfig(m,s)if i then return"Config system unavailable."end if not s then return false,"Please select a config file."end local U=p.Folder..("/settings/"..(s..".json"))if isfile(U)then delfile(U)else return false,"Config file does not exist."end local N=p.Folder.."/settings/autoload.txt"if isfile(N)then local p=readfile(N)if p==s then delfile(N)T:Notify({Title=string.rep(" ",3).."Settings Manager";Description="Autoload file also deleted as it matched the deleted config."})end end return true end function p.SetFolder(m,s)if i then return"Config system unavailable."end p.Folder=s r1()end function p.SaveConfig(m,s)if i then return"Config system unavailable."end if not s then return false,"Please select a config file."end local N=p.Folder..("/settings/"..(s..".json"))local P={objects={}}for p,s in next,p.Options do if not J1[s.Class]then continue end if s.IgnoreConfig then continue end table.insert(P.objects,J1[s.Class].Save(p,s))end local S,e=pcall(U.JSONEncode,U,P)if not S then return false,"Unable to encode into JSON data"end writefile(N,e)return true end function p.LoadConfig(m,s)if i then return"Config system unavailable."end if not s then return false,"Please select a config file."end local N=p.Folder..("/settings/"..(s..".json"))if not isfile(N)then return false,"Invalid file"end local P,S=pcall(U.JSONDecode,U,readfile(N))if not P then return false,"Unable to decode JSON data."end for p,s in next,S.objects do if J1[s.type]then task.spawn(function()J1[s.type].Load(s.flag,s)end)end end return true end function p.RefreshConfigList(s)if i then return"Config system unavailable."end local m=listfiles(p.Folder.."/settings")local U={}for p=1,#m,1 do local s=m[p]if s:sub(-5)==".json"then local p=s:find(".json",1,true)local m=p local N=s:sub(p,p)while N~="/"and(N~="\092"and N~="")do p=p-1 N=s:sub(p,p)end if N=="/"or N=="\092"then local N=s:sub(p+1,m-1)if N~="options"then table.insert(U,N)end end end end return U end G.Enabled=false local l1={}for p,s in pairs(Z)do table.insert(l1,s)end N:PreloadAsync(l1)G.Enabled=true K=true return T end local function T(p)return string.format("%02i",p)end local function G(p)local s=(p-p%60)/60 p=p-s*60 local m=(s-s%60)/60 s=s-m*60 return T(m)..(":"..(T(s)..(":"..T(p))))end task.spawn(function()if not(getgenv()).ExecutedInfoStats then(getgenv()).ExecutedInfoStats=true while task.wait(.95)do pcall(function()local p=(game:GetService("CoreGui")):FindFirstChild("MacLib")if p then local s=p.Base.Sidebar.SidebarGroup.UserInfo.InformationGroup local m=s.UserAndDisplayFrame:FindFirstChild("DisplayName")local U=s.UserAndDisplayFrame:FindFirstChild("Username")local N=s:FindFirstChild("Headshot")if m and(U and N)then local p=os.date("%b %d, %Y")local s=string.format("%.2f ms",(game:GetService("Stats")).Network.ServerStatsItem["Data Ping"]:GetValue())local P=os.date("%H:%M")m.Text=s..(" | "..(p..(" | "..P)))m.TextSize=11 U.Text="RunTime: "..G(time())N.Visible=false end end end)end end end)return p
